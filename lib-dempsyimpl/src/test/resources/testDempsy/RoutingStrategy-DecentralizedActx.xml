<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
   <bean name="routingstrategy" class="com.nokia.dempsy.router.DecentralizedRoutingStrategy">
      <constructor-arg index="0" type="int" value="${total_slots_for_cluster}" />
      <constructor-arg index="1" type="int" value="${min_nodes_for_cluster}" />
   </bean>
</beans>
