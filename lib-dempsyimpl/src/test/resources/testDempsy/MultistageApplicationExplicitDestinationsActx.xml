<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
   <bean class="com.nokia.dempsy.config.ApplicationDefinition">
      <constructor-arg value="test-app" />
      <property name="clusterDefinitions">
         <list>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg value="test-cluster0" />
               <property name="adaptor">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestAdaptor" />
               </property>
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg value="test-cluster1" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
               <property name="destinations" >
               <list>
                 <bean class="com.nokia.dempsy.config.ClusterId" >
                    <constructor-arg index="0" value="test-app" />
                    <constructor-arg index="1" value="test-cluster3" />
                 </bean>
               </list>
               </property>
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg value="test-cluster2" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
               <property name="destinations" >
                 <list>
                 <bean class="com.nokia.dempsy.config.ClusterId" >
                    <constructor-arg index="0" value="test-app" />
                    <constructor-arg index="1" value="test-cluster3" />
                 </bean>
                 <bean class="com.nokia.dempsy.config.ClusterId" >
                    <constructor-arg index="0" value="test-app" />
                    <constructor-arg index="1" value="test-cluster1" />
                 </bean>
                 </list>
               </property>
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg value="test-cluster3" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
            </bean>
         </list>
      </property>
   </bean>
</beans>
