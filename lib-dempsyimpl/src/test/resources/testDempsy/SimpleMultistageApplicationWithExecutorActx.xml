<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>


	<bean name="adaptor" class="com.nokia.dempsy.DempsyTestBase$TestAdaptor" />

   <bean class="com.nokia.dempsy.config.ApplicationDefinition">
      <constructor-arg type="java.lang.String" value="test-app" />
      <property name="executor">
        <bean class="com.nokia.dempsy.executor.DefaultDempsyExecutor" >
           <property name="additionalThreads" value="0" />
           <property name="coresFactor" value="1.0" />
           <property name="maxNumberOfQueuedLimitedTasks" value="123456" />
        </bean>
      </property>
      <property name="clusterDefinitions">
         <list>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg type="java.lang.String" value="test-cluster0" />
               <property name="adaptor" ref="adaptor" />
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg type="java.lang.String" value="test-cluster1" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg type="java.lang.String" value="test-cluster2" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg type="java.lang.String" value="test-cluster3" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
            </bean>
         </list>
      </property>
   </bean>
</beans>
