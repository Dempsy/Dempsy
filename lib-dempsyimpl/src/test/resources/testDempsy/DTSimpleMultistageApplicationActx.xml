<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>


   <bean name="app" class="com.nokia.dempsy.config.ApplicationDefinition">
      <constructor-arg value="test-app" />
   </bean>

	<bean class="com.nokia.dempsy.config.ClusterDefinition">
		<constructor-arg value="test-cluster2" />
		<property name="messageProcessorPrototype">
			<bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
		</property>
		<property name="applicationDefinition" ref="app" />
	</bean>
</beans>
