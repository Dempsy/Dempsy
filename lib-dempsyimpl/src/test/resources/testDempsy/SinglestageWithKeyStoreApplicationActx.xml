<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>


   <bean name="adaptor" class="com.nokia.dempsy.DempsyTestBase$TestAdaptor" />
   <bean name="statcollectorFactory" class="com.nokia.dempsy.monitoring.coda.StatsCollectorFactoryCoda"/>
   <bean class="com.nokia.dempsy.config.ApplicationDefinition">
      <constructor-arg type="java.lang.String" value="test-app" />
      <property name="clusterDefinitions">
         <list>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg type="java.lang.String" value="test-cluster0" />
               <property name="adaptor" ref="adaptor" />
            </bean>
            <bean class="com.nokia.dempsy.config.ClusterDefinition">
               <constructor-arg type="java.lang.String" value="test-cluster1" />
               <property name="messageProcessorPrototype">
                  <bean class="com.nokia.dempsy.DempsyTestBase$TestMp" />
               </property>
               <property name="keySource">
                  <bean class="com.nokia.dempsy.DempsyTestBase.KeySourceImpl">
                     <property name="dempsy" ref="dempsy" />
                     <property name="clusterId">
                       <bean class="com.nokia.dempsy.config.ClusterId">
                         <constructor-arg value="test-app" />
                         <constructor-arg value="test-cluster1" />
                       </bean>
                     </property>
                  </bean>
               </property>
            </bean>
         </list>
      </property>
      <property name="statsCollectorFactory" ref="statcollectorFactory"/>
   </bean>
</beans>
